def fibonacciSearch(arr, x):
    """
    Fungsi Fibonacci Search yang mengembalikan indeks dari elemen x pada array arr,
    atau -1 jika x tidak ada di arr.
    """
    n = len(arr)
    fib2 = 0  # fibonacci(n-2)
    fib1 = 1  # fibonacci(n-1)
    fib = fib2 + fib1  # fibonacci(n)
  
    # Cari nilai dari fibonacci(n) yang terbesar yang lebih kecil atau sama dengan n
    while fib < n:
        fib2 = fib1
        fib1 = fib
        fib = fib2 + fib1
  
    # Indeks awal untuk pencarian
    offset = -1
  
    # Melakukan pencarian
    while fib > 1:
        i = min(offset+fib2, n-1)
  
        # Jika elemen merupakan list, lakukan pencarian di dalamnya
        if isinstance(arr[i], list):
            idx = fibonacciSearch(arr[i], x)
            if idx != -1:
                return [i, idx]

        elif arr[i] < x:
            fib = fib1
            fib1 = fib2
            fib2 = fib - fib1
            offset = i
        elif arr[i] > x:
            fib = fib2
            fib1 = fib1 - fib2
            fib2 = fib - fib1
        else:
            return i
        
    if fib1 and arr[offset+1] == x:
        return offset+1
    return -1


var = ["Arsel", "Avivah", "Daiva", ["Wahyu", "Wibi"]]

cari = input('Cari nama: ')
metode = fibonacciSearch(var, cari)
if metode == -1:
    print('Nama tidak ada')
elif isinstance(metode, list):
    print(f'Nama {cari} terdapat pada indeks: {metode[0]} kolom {metode[1]}')
else:
    print(f'Nama {cari} terdapat pada indeks: {metode}')
